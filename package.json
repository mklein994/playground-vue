{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm build:tokens && pnpm build:tailwind && vite build",
    "build:all-no-tailwind": "pnpm build:tokens && vite build",
    "build:tokens": "node ./sd.config.ts",
    "build:tailwind": "tailwindcss --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck:sd": "vue-tsc --composite false -p ./tsconfig.sd.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:sd && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint . --cache --concurrency auto",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --cache --cache-strategy content --concurrency auto --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --cache --concurrency auto --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --typecheck --isolate --coverage"
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^10.12.0",
    "@sentry/wasm": "^10.12.0",
    "rrweb": "2.0.0-alpha.4",
    "temporal-polyfill": "^0.3.0",
    "vite-plugin-wasm": "^3.5.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "wavesurfer.js": "7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@heroicons/vue": "^2.2.0",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.17",
    "@sentry/vite-plugin": "^4.3.0",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/chroma-js": "^3.1.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/typescript-plugin": "^3.0.7",
    "autoprefixer": "^10.4.21",
    "change-case": "^5.4.4",
    "chroma-js": "^3.1.2",
    "d3": "^7.9.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vue": "^10.4.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.4.0",
    "happy-dom": "^18.0.1",
    "highlight.js": "^11.11.1",
    "modern-normalize": "^3.0.1",
    "postcss": "^8.5.6",
    "postcss-dir-pseudo-class": "^9.0.1",
    "postcss-html": "^1.8.0",
    "postcss-logical": "^8.1.0",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.6.2",
    "sass": "^1.93.0",
    "style-dictionary": "^5.0.4",
    "stylelint": "^16.24.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vitest": "3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "~3.0.7"
  }
}
