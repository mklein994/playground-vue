{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm run build:tokens && pnpm build:tailwind && vite build",
    "build:tokens": "style-dictionary build --config sd.config.js",
    "build:tailwind": "tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --noEmit --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --noEmit --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --noEmit --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --noEmit --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --noEmit --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint .",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^8.17.0",
    "@sentry/wasm": "^8.17.0",
    "rrweb": "2.0.0-alpha.4",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "wavesurfer.js": "7.8.1"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.1.4",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.15",
    "@tailwindcss/forms": "^0.5.7",
    "@types/chroma-js": "^2.4.4",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^2.0.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/typescript-plugin": "^2.0.26",
    "autoprefixer": "^10.4.19",
    "chroma-js": "^2.4.2",
    "d3": "^7.9.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.27.0",
    "fast-glob": "^3.3.2",
    "globals": "^15.8.0",
    "happy-dom": "^14.12.3",
    "highlight.js": "^11.10.0",
    "modern-normalize": "^2.0.0",
    "postcss": "^8.4.39",
    "postcss-dir-pseudo-class": "^8.0.1",
    "postcss-html": "^1.7.0",
    "postcss-logical": "^7.0.1",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.2",
    "sass": "^1.77.7",
    "style-dictionary": "^4.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.3.3",
    "vitest": "^2.0.2",
    "vue-tsc": "~2.0.26"
  },
  "optionalDependencies": {
    "@sentry/vite-plugin": "^2.21.0"
  }
}
