{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext js,ts,vue .",
    "lint:ci": "eslint --ext js,ts,vue --max-warnings 0 . && prettier --check .",
    "format": "eslint --ext js,ts,vue --fix --fix-type layout . && prettier --write ."
  },
  "packageManager": "pnpm@6.20.1",
  "dependencies": {
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@heroicons/vue": "^1.0.5",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@tailwindcss/forms": "^0.4.0",
    "@types/tailwindcss": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "autoprefixer": "^10.4.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "highlight.js": "^11.3.1",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.4.5",
    "postcss-dir-pseudo-class": "^6.0.2",
    "postcss-logical": "^5.0.2",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "tailwindcss": "^3.0.9",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vue-tsc": "^0.30.2"
  }
}
