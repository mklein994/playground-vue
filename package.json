{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm run build:tokens && pnpm build:tailwind && vite build",
    "build:tokens": "style-dictionary build --config sd.config.js",
    "build:tailwind": "tailwindcss --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --noEmit --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --noEmit --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --noEmit --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --noEmit --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --noEmit --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint . --cache",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --cache --cache-strategy content --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --cache --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --isolate --coverage"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^9.1.0",
    "@sentry/wasm": "^9.1.0",
    "rrweb": "2.0.0-alpha.4",
    "vite-plugin-wasm": "^3.4.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "wavesurfer.js": "7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@heroicons/vue": "^2.2.0",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.17",
    "@tailwindcss/cli": "^4.0.8",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/chroma-js": "^3.1.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.6",
    "@vue/test-utils": "^2.4.6",
    "@vue/typescript-plugin": "^2.2.2",
    "apexcharts": "^4.5.0",
    "autoprefixer": "^10.4.20",
    "chroma-js": "^3.1.2",
    "d3": "^7.9.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vue": "^9.32.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.0.0",
    "happy-dom": "^17.1.2",
    "highlight.js": "^11.11.1",
    "modern-normalize": "^3.0.1",
    "postcss": "^8.5.3",
    "postcss-dir-pseudo-class": "^9.0.1",
    "postcss-html": "^1.8.0",
    "postcss-logical": "^8.0.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.1",
    "sass": "^1.85.0",
    "style-dictionary": "^4.3.3",
    "stylelint": "^16.14.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.0.8",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vitest": "^3.0.6",
    "vue-tsc": "~2.2.2",
    "vue3-apexcharts": "^1.8.0"
  },
  "optionalDependencies": {
    "@sentry/vite-plugin": "^3.2.0"
  }
}
