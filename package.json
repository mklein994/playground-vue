{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext js,ts,vue .",
    "lint:ci": "eslint --ext js,ts,vue --max-warnings 0 . && prettier --check .",
    "format": "eslint --ext js,ts,vue --fix --fix-type layout . && prettier --write ."
  },
  "packageManager": "pnpm@6.30.0",
  "dependencies": {
    "@sentry/tracing": "^6.17.6",
    "@sentry/vue": "^6.17.6",
    "@sentry/wasm": "^6.17.6",
    "vue": "^3.2.30",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@heroicons/vue": "^1.0.5",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@tailwindcss/forms": "^0.4.0",
    "@types/node": "^17.0.16",
    "@types/tailwindcss": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/compiler-sfc": "^3.2.30",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "highlight.js": "^11.4.0",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.4.6",
    "postcss-dir-pseudo-class": "^6.0.4",
    "postcss-logical": "^5.0.4",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "vue-tsc": "^0.31.2"
  }
}
