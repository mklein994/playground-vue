{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm run build:tokens && build:tailwind && vite build",
    "build:tokens": "style-dictionary build --config sd.config.cjs",
    "build:tailwind": "tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --noEmit --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --noEmit --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --noEmit --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --noEmit --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --noEmit --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint --ext js,cjs,mjs,ts,vue .",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --ext js,cjs,mjs,ts,vue --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --ext js,cjs,mjs,ts,vue --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "clean": "rm -r dist public/coverage public/tailwind.min.css"
  },
  "packageManager": "pnpm@8.7.4",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^7.66.0",
    "@sentry/wasm": "^7.66.0",
    "rrweb": "2.0.0-alpha.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "wavesurfer.js": "7.2.0"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.0.18",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.10",
    "@tailwindcss/forms": "^0.5.6",
    "@types/d3": "^7.4.0",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-v8": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.15",
    "chroma-js": "^2.4.2",
    "d3": "^7.8.5",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.17.0",
    "fast-glob": "^3.3.1",
    "happy-dom": "^10.11.2",
    "highlight.js": "^11.8.0",
    "modern-normalize": "^2.0.0",
    "postcss": "^8.4.29",
    "postcss-dir-pseudo-class": "^8.0.0",
    "postcss-html": "^1.5.0",
    "postcss-logical": "^7.0.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "style-dictionary": "^3.8.0",
    "stylelint": "^15.10.3",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vue-tsc": "~1.8.8"
  },
  "optionalDependencies": {
    "@sentry/vite-plugin": "^2.7.0"
  }
}
