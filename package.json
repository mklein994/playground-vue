{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm run build:tokens && pnpm build:tailwind && vite build",
    "build:tokens": "style-dictionary build --config sd.config.js",
    "build:tailwind": "tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --noEmit --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --noEmit --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --noEmit --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --noEmit --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --noEmit --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint .",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --isolate --coverage"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^8.37.1",
    "@sentry/wasm": "^8.37.1",
    "rrweb": "2.0.0-alpha.4",
    "vite-plugin-wasm": "^3.3.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "wavesurfer.js": "7.8.8"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.1.5",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.16",
    "@tailwindcss/forms": "^0.5.9",
    "@types/chroma-js": "^2.4.4",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^2.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/typescript-plugin": "^2.1.10",
    "apexcharts": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "chroma-js": "^3.1.2",
    "d3": "^7.9.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.30.0",
    "fast-glob": "^3.3.2",
    "globals": "^15.12.0",
    "happy-dom": "^15.10.0",
    "highlight.js": "^11.10.0",
    "modern-normalize": "^3.0.1",
    "postcss": "^8.4.47",
    "postcss-dir-pseudo-class": "^9.0.1",
    "postcss-html": "^1.7.0",
    "postcss-logical": "^8.0.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "style-dictionary": "^4.1.4",
    "stylelint": "^16.10.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10",
    "vite-plugin-top-level-await": "^1.4.4",
    "vitest": "^2.1.4",
    "vue-tsc": "~2.1.10",
    "vue3-apexcharts": "^1.7.0"
  },
  "optionalDependencies": {
    "@sentry/vite-plugin": "^2.22.6"
  }
}
