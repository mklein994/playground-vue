{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm run build:tokens && pnpm build:tailwind && vite build",
    "build:tokens": "style-dictionary build --config sd.config.js",
    "build:tailwind": "tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --noEmit --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --noEmit --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --noEmit --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --noEmit --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --noEmit --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint .",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --isolate --coverage"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^8.50.0",
    "@sentry/wasm": "^8.50.0",
    "rrweb": "2.0.0-alpha.4",
    "vite-plugin-wasm": "^3.4.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "wavesurfer.js": "7.8.16"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.2.0",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.16",
    "@tailwindcss/forms": "^0.5.10",
    "@types/chroma-js": "^3.1.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/typescript-plugin": "^2.2.0",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "chroma-js": "^3.1.2",
    "d3": "^7.9.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vue": "^9.32.0",
    "fast-glob": "^3.3.3",
    "globals": "^15.14.0",
    "happy-dom": "^16.6.0",
    "highlight.js": "^11.11.1",
    "modern-normalize": "^3.0.1",
    "postcss": "^8.5.1",
    "postcss-dir-pseudo-class": "^9.0.1",
    "postcss-html": "^1.8.0",
    "postcss-logical": "^8.0.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "style-dictionary": "^4.3.0",
    "stylelint": "^16.13.2",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vite-plugin-top-level-await": "^1.4.4",
    "vitest": "^3.0.2",
    "vue-tsc": "~2.2.0",
    "vue3-apexcharts": "^1.8.0"
  },
  "optionalDependencies": {
    "@sentry/vite-plugin": "^3.0.0"
  }
}
