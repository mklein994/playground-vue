{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext js,ts,vue .",
    "lint:ci": "eslint --ext js,ts,vue --max-warnings 0 . && prettier --check .",
    "format": "eslint --ext js,ts,vue --fix --fix-type layout . && prettier --write ."
  },
  "packageManager": "pnpm@6.30.0",
  "dependencies": {
    "@sentry/tracing": "^6.18.2",
    "@sentry/vue": "^6.18.2",
    "@sentry/wasm": "^6.18.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@heroicons/vue": "^1.0.6",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@tailwindcss/forms": "^0.5.0",
    "@types/node": "^17.0.21",
    "@types/tailwindcss": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "highlight.js": "^11.5.0",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.4.12",
    "postcss-dir-pseudo-class": "^6.0.4",
    "postcss-logical": "^5.0.4",
    "prettier": "^2.6.0",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vue-tsc": "^0.33.2-patch.1"
  }
