{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext js,ts,vue .",
    "format": "eslint --ext js,ts,vue --fix --fix-type layout . && prettier --write ."
  },
  "packageManager": "pnpm@6.16.1",
  "dependencies": {
    "vue": "^3.2.16",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@heroicons/vue": "^1.0.4",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@tailwindcss/forms": "^0.3.3",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-vue": "^1.9.1",
    "@vue/compiler-sfc": "^3.2.16",
    "autoprefixer": "^10.3.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "highlight.js": "^11.2.0",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.3.7",
    "postcss-dir-pseudo-class": "^6.0.0",
    "postcss-logical": "^5.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vue-tsc": "^0.3.0"
  }
}
