{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm run build:tokens && pnpm build:tailwind && vite build",
    "build:tokens": "style-dictionary build --config sd.config.cjs",
    "build:tailwind": "tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --noEmit --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --noEmit --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --noEmit --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --noEmit --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --noEmit --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint --ext js,cjs,mjs,ts,vue .",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --ext js,cjs,mjs,ts,vue --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --ext js,cjs,mjs,ts,vue --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "packageManager": "pnpm@8.15.4",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^7.102.0",
    "@sentry/wasm": "^7.102.0",
    "rrweb": "2.0.0-alpha.4",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "wavesurfer.js": "7.7.3"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.1.1",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.13",
    "@tailwindcss/forms": "^0.5.7",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.17",
    "chroma-js": "^2.4.2",
    "d3": "^7.8.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.21.1",
    "fast-glob": "^3.3.2",
    "happy-dom": "^13.4.1",
    "highlight.js": "^11.9.0",
    "modern-normalize": "^2.0.0",
    "postcss": "^8.4.35",
    "postcss-dir-pseudo-class": "^8.0.1",
    "postcss-html": "^1.6.0",
    "postcss-logical": "^7.0.1",
    "postcss-nesting": "^12.0.3",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "style-dictionary": "^3.9.2",
    "stylelint": "^16.2.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "~1.8.27"
  },
  "optionalDependencies": {
    "@sentry/vite-plugin": "^2.14.2"
  }
}
