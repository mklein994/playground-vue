{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext js,ts,vue .",
    "lint:ci": "eslint --ext js,ts,vue --max-warnings 0 . && prettier --check .",
    "format": "eslint --ext js,ts,vue --fix --fix-type layout . && prettier --write ."
  },
  "packageManager": "pnpm@6.20.1",
  "dependencies": {
    "vue": "^3.2.22",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@heroicons/vue": "^1.0.5",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@tailwindcss/forms": "^0.3.4",
    "@types/tailwindcss": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "highlight.js": "^11.3.1",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.3.11",
    "postcss-dir-pseudo-class": "^6.0.0",
    "postcss-logical": "^5.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vue-tsc": "^0.28.10"
  }
}
