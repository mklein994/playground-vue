{
  "name": "playground-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:all": "pnpm run build:tokens && pnpm build:tailwind && vite build",
    "build:tokens": "style-dictionary build --config sd.config.cjs",
    "build:tailwind": "tailwind --minify --input ./src/tailwind.css --output ./public/tailwind.min.css",
    "preview": "vite preview",
    "typecheck:config": "vue-tsc --noEmit --composite false -p tsconfig.vite-config.json",
    "typecheck:workers": "vue-tsc --noEmit --composite false -p tsconfig.workers.json",
    "typecheck:app": "vue-tsc --noEmit --composite false -p tsconfig.app.json",
    "typecheck:tests": "vue-tsc --noEmit --composite false -p tsconfig.vitest.json",
    "typecheck:node-tests": "vue-tsc --noEmit --composite false -p ./tests/tsconfig.node-tests.json",
    "typecheck": "pnpm typecheck:config && pnpm typecheck:workers && pnpm typecheck:tests && pnpm typecheck:node-tests",
    "lint": "eslint --ext js,cjs,mjs,ts,vue .",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:ci": "eslint --ext js,cjs,mjs,ts,vue --max-warnings 0 . && pnpm lint:style && prettier --check .",
    "format": "eslint --ext js,cjs,mjs,ts,vue --fix --fix-type layout . && pnpm lint:style --fix && prettier --write .",
    "test": "vitest",
    "test:node": "node --test --test-reporter=spec",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0",
  "type": "module",
  "dependencies": {
    "@sentry/vue": "^8.7.0",
    "@sentry/wasm": "^8.7.0",
    "rrweb": "2.0.0-alpha.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "wavesurfer.js": "7.7.15"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.1.3",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@observablehq/plot": "^0.6.14",
    "@tailwindcss/forms": "^0.5.7",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/typescript-plugin": "^2.0.19",
    "autoprefixer": "^10.4.19",
    "chroma-js": "^2.4.2",
    "d3": "^7.9.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.26.0",
    "fast-glob": "^3.3.2",
    "happy-dom": "^14.12.0",
    "highlight.js": "^11.9.0",
    "modern-normalize": "^2.0.0",
    "postcss": "^8.4.38",
    "postcss-dir-pseudo-class": "^8.0.1",
    "postcss-html": "^1.7.0",
    "postcss-logical": "^7.0.1",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.0",
    "sass": "^1.77.4",
    "style-dictionary": "^3.9.2",
    "stylelint": "^16.6.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0",
    "vue-tsc": "~2.0.19"
  },
  "optionalDependencies": {
    "@sentry/vite-plugin": "^2.17.0"
  }
}
